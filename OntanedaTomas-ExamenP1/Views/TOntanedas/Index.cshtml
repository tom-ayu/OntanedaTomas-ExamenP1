@model IEnumerable<OntanedaTomas_ExamenP1.Models.TOntaneda>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VelocidadCPU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Marca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstaOverclokeado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaObtencion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Celular)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VelocidadCPU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstaOverclokeado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaObtencion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Celular.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCPU">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCPU">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCPU">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
